        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Search SLO signals: request rate, error rate, and latency distribution.",
  "editable": false,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [
    {
      "asDropdown": false,
      "icon": "external link",
      "includeVars": false,
      "keepTime": false,
      "tags": [],
      "targetBlank": true,
      "title": "Monitoring + alerting playbook",
      "type": "link",
      "url": "https://github.com/jerdaw/healtharchive-backend/blob/main/docs/operations/playbooks/monitoring-and-alerting.md"
    }
  ],
  "panels": [
    {
      "datasource": "prometheus",
      "description": "PromQL: rate(healtharchive_search_requests_total[5m])",
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "displayMode": "hidden",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(healtharchive_search_requests_total[5m])",
          "refId": "A"
        }
      ],
      "title": "Search Requests (req/s)",
      "type": "timeseries"
    },
    {
      "datasource": "prometheus",
      "description": "PromQL: rate(errors)/rate(requests) over 5m",
      "fieldConfig": {
        "defaults": {
          "max": 1,
          "min": 0,
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "displayMode": "hidden",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(healtharchive_search_errors_total[5m]) / clamp_min(rate(healtharchive_search_requests_total[5m]), 1e-9)",
          "refId": "A"
        }
      ],
      "title": "Search Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": "prometheus",
      "description": "PromQL: histogram_quantile(p, sum by (le) (rate(healtharchive_search_duration_seconds_bucket[5m])))",
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le) (rate(healtharchive_search_duration_seconds_bucket[5m])))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(healtharchive_search_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le) (rate(healtharchive_search_duration_seconds_bucket[5m])))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "title": "Search Latency (histogram quantiles)",
      "type": "timeseries"
    },
    {
      "datasource": "prometheus",
      "description": "PromQL: rate(healtharchive_search_mode_total[5m]) by mode",
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 18
      },
      "id": 4,
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum by (mode) (rate(healtharchive_search_mode_total[5m]))",
          "legendFormat": "{{mode}}",
          "refId": "A"
        }
      ],
      "title": "Search Mode Breakdown (req/s)",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
